{
  "name": "madui",
  "version": "1.0.0",
  "description": "A collection of reusable components for building user interfaces with Tailwind CSS and Radix UI.",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "madui",
    "url": "https://twitter.com/madui"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Anas-github-acc/madui.git",
    "directory": "packages/madui"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "components",
    "ui",
    "tailwind",
    "radix-ui",
    "madui"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./registry": {
      "types": "./dist/registry/index.d.ts",
      "default": "./dist/registry/index.js"
    }
  },
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch --sourcemap --dts --clean",
    "build": "tsup --sourcemap --dts --clean",
    "clean": "rimraf dist && rimraf components",
    "start": "node dist/index.js",
    "start:dev": "cross-env REGISTRY_URL=http://localhost:3001/r node dist/index.js",
    "start:prod": "cross-env REGISTRY_URL=http://localhost:3001/r node dist/index.js",
    "test": "vitest run",
    "generate:component": "turbo gen react-component",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/parser": "^7.28.4",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@types/react": "19.1.4",
    "@types/stringify-object": "^4.0.5",
    "eslint": "9.27.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "recast": "^0.23.11",
    "stringify-object": "^6.0.0",
    "tailwindcss": "^4.1.14",
    "tsup": "^8.5.0",
    "typescript": "^5.5.3",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "@antfu/ni": "^25.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/deepmerge": "^2.2.3",
    "@types/fs-extra": "^11.0.4",
    "@types/prompts": "^2.4.9",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "commander": "^14.0.1",
    "cosmiconfig": "^9.0.0",
    "cross-env": "^10.1.0",
    "deepmerge": "^4.3.1",
    "execa": "^9.6.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "kleur": "^4.1.5",
    "lucide-react": "0.511.0",
    "ni": "^0.0.2",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "3.3.0",
    "ts-morph": "^27.0.0",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.41.0",
    "zod": "^3.25.4"
  }
}
